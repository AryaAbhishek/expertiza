<table id="myTable" class="tbl_heat tablesorter" width="100%" cellspacing='0' cellpadding='2' border='0'>
  <thead>
  <tr bgcolor='#CCCCCC'>
    <!-- class value decides whether the column should be sortable or not  -->
    <th width="30%" class="sorter-true">Team <span></span></th>
    <th width="30%" class="sorter-false">Reviewers <span></span></th>
    <th width="40%" class="sorter-true">Metrics <span></span></th>
  </tr>
  </thead>
  <tbody>
  max Scores:<% max_scores= get_max_assignment_review_score_per_round(@id) %>
  <% max_scores.each do |score|%>
  <%=score %>
  <% end%>
  <%@reviewers.keys.each do |team_name| %>
    <tr>
      <td>
        Team Name:<% team_name %><br>
        Team Members:<br>
        <% members=get_team_members_by_team_name(team_name)%>
        <% members.each do |member|%>
          <%=User.find(member.user_id).fullname %><br>
        <% end %>
      </td>
      <td>
        <%question_answers = get_reviews_score_for_team(@id, team_name) %>
        <% i = 1 %>
        <% question_answers.each do |question_answer| %>
          <div style="border-top: solid; border-width: 1px;">
            <%= "Round: " + i.to_s %>
            <% question_answer.each do |reviewer, answer| %>
              <div>
                <% response_id = get_response_id(reviewer, @teams[team_name], @id) %>
                <%= link_to "("+@reviewers[team_name.to_s][reviewer.to_s].to_s+")", :controller => 'popup', :action => 'team_users_popup', :id => @teams[team_name], :id2 => response_id, :assignment_id=>@id %>
                <%= list_review_submissions(reviewer, @teams[team_name], response_id) %>
                <%= list_hyperlink_submission(response_id, 5386) if Assignment.find_by(id: @id.to_i).course.instructor.name == 'Jkidd' %>
                <%= ": " + answer.to_s %>
              </div>
            <% end %>
          </div>
          <% i+=1 %>
        <% end %>
      </td>
      <td>

      </td>
    </tr>
  <% end %>
  </tbody>
</table>
